{
  "outputFormatters": [
    [
      "markdownlint-cli2-formatter-json"
    ]
  ],
  "config": {
    "default": true,
    "line-length": false,
    "no-duplicate-heading": {
      "siblings_only": true
    },
    "no-trailing-punctuation": {
      "punctuation": ".,;:"
    },
    "ol-prefix": true,
    "list-marker-space": false,
    "no-inline-html": true,
    "first-line-h1": true,
    "no-trailing-spaces": true,
    "single-h1": true,
    "blank_lines": true
  }
}
